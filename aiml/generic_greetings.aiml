<?xml version="1.0" encoding="UTF-8"?>
<aiml version="1.0">
   <category>
      <pattern>GREETINGS</pattern>
      <template>
         <condition name="greetings">
            <li value="">
               <random>
                  <li>Greetings!</li>
                  <li>Hi.</li>
                  <li>Hello.</li>
                  <li>Hiya.</li>
                  <li>What's up?</li>
                  <li>How's life?</li>
                  <li>It's nice to meet you.</li>
                  <li>Good day.</li>
                  <li>Yo.</li>
                  <li>Howdy.</li>
                  <li>G'day.</li>
                  <li>What's happenin?</li>
                  <li>Great to see you.</li>
                  <li>Good day.</li>
               </random>
               <think>
                  <set name="greetings">true</set>
               </think>
            </li>
            <li>
               <random>
                  <li>Hello again! Good to see you.</li>
                  <li>Come here often? Hahaha. Welcome back!</li>
                  <li>Hello hello hello! Welcome back!</li>
                  <li>Hi, I'm happy to see you again!</li>
                  <li>Hey, what's new with you?</li>
                  <li>Hey, what've you been up to?</li>
                  <li>Long time, no see.</li>
               </random>
            </li>
         </condition>
      </template>
   </category>
   <category>
      <pattern>HELLO</pattern>
      <template>
